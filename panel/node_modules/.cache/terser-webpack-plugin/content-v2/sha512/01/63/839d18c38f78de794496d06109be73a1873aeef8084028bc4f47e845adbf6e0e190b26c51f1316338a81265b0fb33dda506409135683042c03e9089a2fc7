ed={isAuthenticated} />\n          </Route>\n          <Route path=\"/servers\">\n            <Servers isAuthenticated={isAuthenticated} />\n          </Route>\n          <Route path=\"/dashboard/:id\">\n            <Dashboard />\n          </Route>\n          <Route path=\"/logout\">\n            <Logout />\n          </Route>\n        </Switch>\n        <Footer />\n      </div>\n    </Router>\n  );\n}\n\nexport default App;","import React from 'react';\nimport ReactDOM from 'react-dom';\nimport App from './App';\nimport './index.css';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);"]}}